<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.Insets?>

<TableView xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           prefHeight="400.0" prefWidth="600.0"
           fx:id="tvZamowieniaSzcz">
    <padding><Insets top="10" right="10" bottom="10" left="10"/></padding>
    <columns>
        <TableColumn fx:id="tvZamowieniaSzczId" text="ID"/>
        <TableColumn fx:id="tvZamowieniaSzczProdId" text="ID Produktu"/>
        <TableColumn fx:id="tvZamowieniaSzczProd" text="Nazwa Produktu"/>
        <TableColumn fx:id="tvZamowieniaSzczIlosc" text="Ilość"/>
    </columns>
</TableView>
